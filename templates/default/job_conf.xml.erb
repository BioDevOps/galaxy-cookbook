<?xml version="1.0"?>
<job_conf>
    <plugins>
        <plugin id="sge" type="runner" load="galaxy.jobs.runners.drmaa:DRMAAJobRunner" worker="<%= node[:galaxy][:cluster][:drmaa][:numberofworker] %>">
            <param id="drmaa_library_path"><%= node[:galaxy][:cluster][:drmaa][:drmaalibrarypath] %></param>
        </plugin>
    </plugins>
    <handlers default="handlers">
        <handler id="main" tags="handlers"/>
    </handlers>
    <destinations default="sge_default">
        <destination id="sge_default" runner="drmaa"/>
    </destinations>
</job_conf>
